{
   "name": "Create Link",
   "version": "0.5.7",
   "manifest_version": 3,
   "action": {
      "default_icon": "icon64.png",
      "default_title": "Create Link",
      "default_popup": "popup.html"
   },
   "description": "Copy current page URL to clipboard in various formats.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "64": "icon64.png"
   },
   "commands": {
     "_execute_action": {
       "suggested_key": {
          "default": "Alt+C"
       }
     }
   },
   "host_permissions": [
     "http://*/*",
     "https://*/*"
   ]
}
